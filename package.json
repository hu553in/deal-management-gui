{
  "name": "deal-management-gui",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "0.21.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-modal": "3.12.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "react-select": "3.1.1",
    "styled-components": "5.2.1",
    "web-vitals": "0.2.4"
  },
  "devDependencies": {
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.3.0",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "react-app-rewire-alias": "0.1.9",
    "react-app-rewired": "2.1.8"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
